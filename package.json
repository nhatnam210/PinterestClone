{
    "name": "pinterestclone",
    "version": "1.0.0",
    "description": "web clone for JS project",
    "main": "index.js",
    "scripts": {
        "watch": "node-sass --watch interface/scss --output interface/css",
        "formatGit": "lint-staged --allow-empty",
        "formatCode": "prettier --single-quote --trailing-comma all --tab-width 4 --write ./**/*.{js,json,css,scss}",
        "start": "nodemon --inspect src/index.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nhatnam210/PinterestClone.git"
    },
    "keywords": [
        "MongoDB",
        "ExpressJS",
        "ReactJS",
        "NodeJs"
    ],
    "author": "Naw + team 4",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/nhatnam210/PinterestClone/issues"
    },
    "homepage": "https://github.com/nhatnam210/PinterestClone#readme",
    "dependencies": {
        "express": "^4.17.3"
    },
    "devDependencies": {
        "husky": "^7.0.0",
        "lint-staged": "^12.3.4",
        "node": "^17.4.0",
        "node-sass": "^7.0.1",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "sass": "^1.49.8"
    },
    "lint-staged": {
        "./**/*.{js,json,css,scss}": "prettier --single-quote --trailing-comma all --tab-width 4 --write"
    }
}
